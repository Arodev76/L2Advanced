<?xml version='1.0' encoding='utf-8'?>
<!ELEMENT list (map)*>
<!ELEMENT map (parameter|objects)*>
<!ATTLIST map
	name CDATA #REQUIRED>
<!ELEMENT parameter (#PCDATA)>
<!ATTLIST parameter
	name CDATA #REQUIRED
	value CDATA #REQUIRED>
<!ELEMENT objects (door|point|zone)*>
<!ATTLIST objects
	name CDATA #REQUIRED
	team CDATA #IMPLIED
	index CDATA #IMPLIED>
<!ELEMENT door (#PCDATA)>
<!ATTLIST door
	id CDATA #REQUIRED>
<!ELEMENT zone (#PCDATA)>
<!ATTLIST zone
	name CDATA #REQUIRED>
<!ELEMENT point (#PCDATA)>
<!ATTLIST point
	x CDATA #REQUIRED
	y CDATA #REQUIRED
	z CDATA #REQUIRED
	h CDATA #IMPLIED>